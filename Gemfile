  gem "jekyll-sitemap"
  gem "jekyll-feed"
  gem "jekyll-seo-tag"
  gem 'jekyll-imagemagick'
  gem 'jekyll-minifier'
  gem 'jekyll_dynamic_assets'
  gem 'jekyll-terser', :git => "https://github.com/RobertoJBeltran/jekyll-terser.git"
end